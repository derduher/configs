{
    //unique to node package
    "globals": {
        "_": true,
        "EDGE": true,
        "Backbone": true,
        "Modernizr": true,
        "jasmine": false,
        "spyOn": false,
        "it": false,
        "devel": true,
        "browser": true,
        "describe": false,
        "expect": false,
        "beforeEach": false,
        "waits": false,
        "waitsFor": false,
        "runs": false,
        "jquery": true
    },

        "devel": true,
        "browser": true,
        "jquery": true,


//relaxing
    "boss" : false, //prohibit assignments where expressions are expected
    "debug": false, //suppress debugger statements
    "eqnull": true, //suppress warnings about comparisons with null/undef
    "es5" : false,
    "evil": false, //evals are bad don't supress 
    "laxbreak": false,
    "laxcomma": true, //allow comma first style
    "sub": true, //suppress warning about using [ ]when .prop is just as good

    //enforcing
    "bitwise": true,
    "curly": false, //ideally true
    "eqeqeq": false, //ideally true
    "forin": false,
    "immed": true, // immediate function invocation
    "indent": 4,
    "latedef": true, //prohibit late definition
    "maxparams": 5,
    "newcap": true, // have to cap classes
    "noarg": true,
    "noempty": false,
    "nonew": true, // if you aren't assigning it why call it.
    "plusplus": false, // plus plus is ok keep it
    "regexp": true, //prohibits unsafe dot
    "undef": true, // preventing typos
    "unused": false, // really needed for debugging
    "strict": false, //force ecma5
    "trailing": true
}
